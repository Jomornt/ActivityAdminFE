n  props: {\r\n    editInfo: Object,\r\n    editVisible: Boolean\r\n  },\r\n  data () {\r\n    return {\r\n      copyEditVisible: this.editVisible\r\n    }\r\n  },\r\n  methods: {\r\n    handleCancel () {\r\n      this.copyEditVisible = false\r\n      this.$emit('editVisibleChange')\r\n    },\r\n    handleSure () {\r\n      let _this = this\r\n      let data = {\r\n        account: this.editInfo.account,\r\n        password: this.editInfo.password,\r\n        nickname: this.editInfo.nickname,\r\n        intro: this.editInfo.intro,\r\n        gender: this.editInfo.gender,\r\n        birth: this.editInfo.birth,\r\n        status: this.editInfo.status,\r\n        tips: this.editInfo.tips\r\n      }\r\n      this.axios({\r\n        method: 'put',\r\n        url: 'http://localhost:8080/Woola_admin_war_exploded/user',\r\n        headers: {\r\n          'Content-Type': 'application/x-www-form-urlencoded'\r\n        },\r\n        data: Qs.stringify(data)\r\n      }).then((response) => {\r\n        _this.$message({\r\n          showClose: true,\r\n          message: '保存成功',\r\n          type: 'success'\r\n        })\r\n        _this.$emit('tblChange')\r\n        _this.handleCancel()\r\n      }).catch((error) => {\r\n        console.log(error)\r\n        _this.$message({\r\n          showClose: true,\r\n          message: '保存失败',\r\n          type: 'fail'\r\n        })\r\n      })\r\n    }\r\n  },\r\n  watch: {\r\n    editVisible: function () {\r\n      this.copyEditVisible = this.editVisible\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/pages/User"}]}