orm-item label=\"个人介绍\">\r\n        <template>\r\n          <el-input\r\n            type=\"textarea\"\r\n            :autosize=\"{ minRows: 2, maxRows: 4}\"\r\n            v-model=\"editInfo.intro\">\r\n          </el-input>\r\n        </template>\r\n      </el-form-item>\r\n      <el-form-item label=\"生日\">\r\n        <el-date-picker\r\n          v-model=\"editInfo.birth\"\r\n          type=\"date\"\r\n          placeholder=\"选择日期\">\r\n        </el-date-picker>\r\n      </el-form-item>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"handleCancel\">取 消</el-button>\r\n      <el-button type=\"primary\" @click=\"handleSure\">确 定</el-button>\r\n    </div>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport Qs from 'qs'\r\nexport default {\r\n  name: 'EditDialog',\r\n  props: {\r\n    editInfo: Object,\r\n    editVisible: Boolean\r\n  },\r\n  data () {\r\n    return {\r\n      copyEditVisible: this.editVisible\r\n    }\r\n  },\r\n  methods: {\r\n    handleCancel () {\r\n      this.copyEditVisible = false\r\n      this.$emit('editVisibleChange')\r\n    },\r\n    handleSure () {\r\n      let _this = this\r\n      let data = {\r\n        account: this.editInfo.account,\r\n        password: this.editInfo.password,\r\n        nickname: this.editInfo.nickname,\r\n        intro: this.editInfo.intro,\r\n        gender: this.editInfo.gender,\r\n        birth: this.editInfo.birth,\r\n        status: this.editInfo.status,\r\n        tips: this.editInfo.tips\r\n      }\r\n      this.axios({\r\n        method: 'put',\r\n        url: 'http://localhost:8080/Woola_admin_war_exploded/user',\r\n        headers: {\r\n          'Content-Type': 'application/x-www-form-urlencoded'\r\n        },\r\n        data: Qs.stringify(data)\r\n      }).then((response) => {\r\n        _this.$message({\r\n          showClose: true,\r\n          message: '保存成功',\r\n          type: 'success'\r\n        })\r\n        _this.$emit('tblChange')\r\n   