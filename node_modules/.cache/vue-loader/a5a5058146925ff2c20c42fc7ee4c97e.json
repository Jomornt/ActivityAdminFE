agination\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, prev, pager, next, jumper\"\r\n        :total=\"pageInfo.total\">\r\n      </el-pagination>\r\n    </div>\r\n    <black-dialog\r\n      :editInfo=\"editInfo\"\r\n      :blackInfo=\"blackInfo\"\r\n      :blackVisible=\"blackVisible\"\r\n      @blackVisibleChange=\"handleBlackVisibleChange\"\r\n      @tblChange=\"handleTblChange\"\r\n    >\r\n    </black-dialog>\r\n    <edit-dialog\r\n      :editInfo=\"editInfo\"\r\n      :editVisible=\"editVisible\"\r\n      @editVisibleChange=\"handleEditVisibleChange\"\r\n      @tblChange=\"handleTblChange\"\r\n      ></edit-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport EditDialog from '@/pages/User/EditDialog'\r\nimport BlackDialog from '@/pages/User/BlackDialog'\r\nimport { ShowMessage } from '@/common/js/util'\r\nexport default {\r\n  name: 'UserTable',\r\n  components: {\r\n    EditDialog,\r\n    BlackDialog\r\n  },\r\n  props: {\r\n    status: String,\r\n    blackInfo: Object\r\n  },\r\n  data () {\r\n    return {\r\n      pageInfo: Object,\r\n      blackVisible: false,\r\n      editVisible: false,\r\n      currentPage: 1,\r\n      currentAccount: '111',\r\n      pageSize: 8,\r\n      editInfo: {\r\n        account: '',\r\n        password: '',\r\n        nickname: '',\r\n        intro: '',\r\n        gender: '',\r\n        birth: '',\r\n        status: '',\r\n        tips: 0\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    handleEdit (row) {\r\n      console.log(row.account)\r\n      this.currentAccount = row.account\r\n      this.editVisible = true\r\n      this.axios.get(`http://localhost:8080/Woola_admin_war_exploded/user/${this.currentAccount}`\r\n      ).then((response) => {\r\n        var data = response.data.data\r\n        this.editInfo = data.userInfo[0]\r\n      }).catch((error) => {\r\n        console.log(error)\r\n      })\r\n    },\r\n